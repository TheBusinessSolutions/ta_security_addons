<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.digest</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='contacts_settings']" position="before">
                    <block title="Scan Type" groups="adevx_base_barcode_scanning.group_barcode_label_scan_type">
                        <setting class="col-12 col-lg-6 o_setting_box"
                                 help="Set Purchase, Sale and Supplier Reference RequiredPicking Scan Type.">
                            <field name="scan_type" help="Purchase, Sale and Picking Scan Type Tool Tip"/>
                        </setting>
                    </block>
                </xpath>
            </field>
        </record>


    </data>
</odoo>